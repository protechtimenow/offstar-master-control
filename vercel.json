{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "GITHUB_TOKEN": "@github_token",
    "WALLET_ADDRESS": "0x21cC30462B8392Aa250453704019800092a16165"
  }
}